- append_javascript_pack_tag('findings')

= form_with(model: item, remote: true, html: { id: "new_item_form" }) do |f|
  .mb-3
    = f.label :name
    = f.text_field :name, class: "form-control"
  .mb-3
    = f.label :description
    = f.text_area :description, class: "form-control"
  = f.submit "Save", class: "btn btn-success"
